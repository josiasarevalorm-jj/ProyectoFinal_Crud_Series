QLabel {
    color: rgb(220, 220, 220);
    font: bold 11pt &quot;Arial&quot;;
}

/* ===== BOTONES ===== */
QPushButton {
    background-color: rgb(40, 40, 40);
    color: white;
    font: bold 11pt &quot;Arial Black&quot;;
    border-radius: 18px;
    padding: 10px;
    text-align: center;
    padding-left: 20px;
}

/* ===== BOTONES HOVER ===== */
QPushButton:hover {
    background-color: rgb(229, 9, 20);
    color: white;
}

/* ===== BOTONES PRESIONADOS ===== */
QPushButton:pressed {
    background-color: rgb(180, 8, 16);
}

/* ===== INPUTS ===== */
